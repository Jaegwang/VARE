export LD_LIBRARY_PATH=/lib64:/usr/lib64:/netapp/backstage/pub/lib/extern/lib:/netapp/backstage/pub/lib/zelos/lib:/netapp/backstage/pub/lib/extern/cuda/lib64:/opt/pixar/RenderManProServer-22.3/lib:/home/wanho.choi/work/Bora/build/lib;

